<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.djsoft.djalarm">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <service android:name=".DJA_ALARM_SERVICE" />
        <activity android:name=".DJA_MAIN">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DJA_SET_ALARM" />
        <activity android:name=".DJA_STOP_ALARM"
            android:configChanges="orientation|screenSize"/>
        <activity android:name=".DJA_SETTINGS"></activity>
    </application>
</manifest>